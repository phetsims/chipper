{
  "name": "chipper",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/chipper.git"
  },
  "scripts": {
    "test": "qunit"
  },
  "phet": {
    "chipperSupportsOutputJSGruntTasks": true,
    "supportsOutputJS": true
  },
  "devDependencies": {
    "@babel/core": "~7.21.4",
    "@babel/plugin-proposal-decorators": "~7.21.0",
    "@babel/preset-env": "~7.21.4",
    "@babel/preset-react": "~7.18.6",
    "@babel/preset-typescript": "~7.21.4",
    "@types/jquery": "~3.5.13",
    "@types/lodash": "~4.14.172",
    "@types/p2": "~0.7.39",
    "@types/pako": "~2.0.0",
    "@types/qunit": "~2.19.6",
    "@types/three": "~0.137.0",
    "@types/react": "~18.0.12",
    "@types/react-dom": "~18.0.5",
    "@typescript-eslint/parser": "~6.1.0",
    "@typescript-eslint/eslint-plugin": "~6.1.0",
    "@typescript-eslint/utils": "~6.1.0",
    "@webgpu/types": "~0.1.34",
    "archiver": "~5.3.0",
    "axios": "~0.21.4",
    "@babel/eslint-parser": "~7.19.1",
    "docdash": "~1.2.0",
    "eslint": "~8.28.0",
    "eslint-plugin-react": "~7.31.11",
    "expose-loader": "~4.1.0",
    "grunt": "~1.5.3",
    "html-webpack-plugin": "~5.3.2",
    "jimp": "~0.16.1",
    "jpeg-js": "~0.4.3",
    "jsdoc": "~3.6.7",
    "lodash": "~4.17.21",
    "marked": "~4.0.1",
    "node-html-encoder": "~0.0.2",
    "pngjs": "~6.0.0",
    "puppeteer": "~19.2.2",
    "qunit": "~2.20.0",
    "taffydb": "~2.7.3",
    "terser": "~4.6.4",
    "typescript": "~5.1.3",
    "webpack": "~5.76.3",
    "webpack-cli": "~5.0.0",
    "webpack-dev-server": "~4.11.1"
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/phet-library_eslintrc.js",
    "parser": "../chipper/node_modules/@babel/eslint-parser",
    "ignorePatterns": [
      "dist/",
      "node_modules/"
    ],
    "rules": {
      "bad-chipper-text": "error"
    },
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "parser": "@typescript-eslint/parser"
      }
    ]
  }
}
