  "devDependencies": {
    "@babel/core": "~7.15.0",
    "@babel/preset-env": "~7.15.0",
    "@types/lodash": "~4.14.172",
    "@types/qunit": "~2.11.2",
    "@typescript-eslint/parser": "~4.33.0",
    "archiver": "~5.3.0",
    "axios": "~0.21.4",
    "babel-eslint": "~10.1.0",
    "docdash": "~1.2.0",
    "eslint": "~7.32.0",
    "eslint-plugin-react": "~7.25.1",
    "grunt": "~1.4.1",
    "html-webpack-plugin": "~5.3.2",
    "jimp": "~0.16.1",
    "jpeg-js": "~0.4.3",
    "jsdoc": "3.6.7",
    "lodash": "~4.17.21",
    "marked": "~3.0.2",
    "node-html-encoder": "~0.0.2",
    "pngjs": "~6.0.0",
    "puppeteer": "~19.2.2",
    "qunit": "~2.16.0",
    "taffydb": "~2.7.3",
    "terser": "~4.6.4",
    "typescript": "~4.4.2",
    "webpack": "~5.75.0",
    "webpack-cli": "~5.0.0",
    "webpack-dev-server": "~4.11.1"
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "parser": "../chipper/node_modules/babel-eslint",
    "ignorePatterns": [
      "dist/",
      "node_modules/"
    ],
    "rules": {
      "bad-chipper-text": "error"
    },
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true
    }
  }
}
