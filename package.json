{
  "name": "chipper",
  "version": "2.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/chipper.git"
  },
  "phet": {
    "chipperSupportsOutputJSGruntTasks": true,
    "supportsOutputJS": true
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "7.16.0",
    "@babel/preset-typescript": "7.16.0",
    "@types/jquery": "^3.5.13",
    "@types/lodash": "^4.14.172",
    "@types/p2": "^0.7.39",
    "@types/pako": "2.0.0",
    "@types/qunit": "^2.11.2",
    "@types/three": "^0.137.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/parser": "5.30.5",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/utils": "5.30.5",
    "archiver": "^5.3.0",
    "axios": "^0.21.4",
    "@babel/eslint-parser": "7.18.2",
    "docdash": "^1.2.0",
    "eslint": "8.19.0",
    "eslint-plugin-react": "7.30.1",
    "grunt": "~1.5.3",
    "html-webpack-plugin": "^5.3.2",
    "jimp": "^0.16.1",
    "jpeg-js": "^0.4.3",
    "jsdoc": "3.6.7",
    "lodash": "^4.17.21",
    "marked": "^4.0.1",
    "node-html-encoder": "^0.0.2",
    "pngjs": "^6.0.0",
    "puppeteer": "~13.5.2",
    "qunit": "^2.16.0",
    "taffydb": "^2.7.3",
    "terser": "^4.6.4",
    "typescript": "^4.7.4",
    "webpack": "^5.51.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0"
  },
  "eslintConfig": {
    "extends": "../chipper/eslint/sim_eslintrc.js",
    "parser": "../chipper/node_modules/@babel/eslint-parser",
    "ignorePatterns": [
      "dist/",
      "node_modules/"
    ],
    "rules": {
      "bad-chipper-text": "error"
    },
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": true,
      "requireConfigFile": false
    },
    "overrides": [
      {
        "files": [
          "**/*.ts",
          "**/*.tsx"
        ],
        "parser": "@typescript-eslint/parser"
      }
    ]
  }
}
